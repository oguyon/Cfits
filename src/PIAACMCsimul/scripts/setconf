#!/bin/bash

# example script to set coronagraph parameters


ln -s /home/olivier/src/Cfits/bin/cfitsTK $PWD/PIAACMCdesign

./runPIAACMCdesign -e 3
./runPIAACMCdesign -a
echo "2" > conf/conf_extmode.txt
echo "20" > conf/conf_ssize.txt

echo "1.30" > conf/conf_fpmradld.txt
echo "2.00" > conf/conf_maskradld.txt

echo "0" > searchtime.txt

